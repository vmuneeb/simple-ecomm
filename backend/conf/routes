->              /             portal.Routes
->              /api                api.Routes
->              /admin              admin.Routes

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public/assets/portal/", file: Asset)

GET     /asset/admin/*file               controllers.Assets.versioned(path="/public/assets/admin/", file: Asset)
